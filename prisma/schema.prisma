generator client {
  provider     = "prisma-client-js"
  moduleFormat = "cjs"
}

datasource db {
  provider = "mysql"
}

model notes {
  id    Int     @id @default(autoincrement())
  name String 
  description String
  created_at DateTime @default(now())
  updated_at DateTime @default(now())
}
